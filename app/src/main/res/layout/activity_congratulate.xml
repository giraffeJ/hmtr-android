<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="30dp"
    android:gravity="center_horizontal">


    <TextView
        android:textColor="#909090"
        android:textSize="12dp"
        android:layout_marginBottom="200dp"
        android:layout_gravity="center"
        android:id="@+id/congratulate_script_tv"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text=""/>


    <ImageView
        android:onClick="onClick_welcome_img"
        android:layout_gravity="center"
        android:id="@+id/congratulate_charactor_iv"
        android:layout_width="170dp"
        android:layout_height="240dp" />

    <Button
        android:visibility="invisible"
        android:background="@drawable/round_purple_background"
        android:id="@+id/congratulate_btn"
        android:layout_marginTop="190dp"
        android:layout_gravity="center"
        android:onClick="onClick_intro_congratulate"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:text="결과 확인하기"
        android:textColor="#ffffff" />

</FrameLayout>