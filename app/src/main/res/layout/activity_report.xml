                android:layout_marginLeft="4dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#909090"
                android:background="@drawable/round_gray_background"
                android:text=""/>

            <LinearLayout
                android:layout_marginTop="40dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_gravity="center_vertical"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@drawable/purple_circle"/>

                <TextView
                    android:layout_marginLeft="4dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="16dp"
                    android:textColor="#909090"
                    android:text="진학상황"/>
            </LinearLayout>
            <TextView
                android:lineSpacingMultiplier="1.1"
                android:layout_marginTop="5dp"
                android:id="@+id/report_entrance_comment_tv"
                android:padding="8dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="4dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#909090"
                android:background="@drawable/round_gray_background"
                android:text=""
                android:layout_marginBottom="70dp"/>


            <Button
                android:layout_marginTop="100dp"
                android:background="#7c5bee"
                android:textColor="#ffffff"
                android:layout_gravity="bottom"
                android:text="보고서 저장"
                android:textSize="18dp"
                android:onClick="onClick_report_save"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="50dp"/>

            <Button
                android:layout_marginTop="30dp"
                android:background="#7c5bee"
                android:textColor="#ffffff"
                android:layout_gravity="bottom"
                android:text="앱 초기화"
                android:textSize="18dp"
                android:onClick="onClick_report_delete"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="50dp"/>

            <!--저작권표시-->
            <TextView
                android:layout_marginTop="80dp"
                android:textColor="@color/reportPurple"
                android:textSize="12dp"
                android:text="COPYRIGHT(c) 2013 HUMENTORY ALL RIGHTS RESERVED"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
